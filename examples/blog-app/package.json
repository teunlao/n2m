{
  "name": "@n2m/blog-app",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts tsx --watch src/payload/server.default.ts",
    "generate": "drizzle-kit generate",
    "migrate": "drizzle-kit migrate",
    "db:seed": "tsx src/api/db/seed.ts",
    "postinstall": "pnpm run theme",
    "stress-test:csr": "autocannon -c 20 -p 1 -d 100 http://localhost:6554?shell=csr",
    "stress-test:ssg": "autocannon -c 20 -p 1 -d 100 http://localhost:6554?shell=ssg",
    "stress-test:ssr": "autocannon -c 20 -p 1 -d 10 http://localhost:6554",
    "theme": "chakra typegen src/webapp/theme/theme.ts",
    "theme:watch": "chakra typegen src/webapp/theme/theme.ts --watch",
    "server:playground": "tsx watch src/server/playground.tsx",
    "debug": "PRERENDER_SECRET=pass node --expose-gc --inspect ../../node_modules/vite/bin/vite.js --host --port 5554 --open",
    "debug:mocks": "MOCKS_ENABLED=1 CUSTOM_MOCKED_PATHS=* node --inspect node_modules/vite/bin/vite.js --host --port 6554 --open",
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts vite dev --host --port 5554",
    "build": "rimraf dist && pnpm run build.client && pnpm run build.server",
    "build.client": "NODE_ENV=production VITE_PRERENDER_SECRET=pass vite build --mode production",
    "build.server": "NODE_ENV=production VITE_PRERENDER_SECRET=pass vite build --ssr --mode production",
    "start": "NODE_ENV=production PORT=6554 node --expose-gc --inspect dist/server.js",
    "start:mocks": "NODE_ENV=production MOCKS_ENABLED=1 CUSTOM_MOCKED_PATHS=* PORT=5554 node --expose-gc --inspect dist/server.js",
    "typecheck": "tsc --noEmit",
    "----------payload cms related scripts----------": "",
    "payload:dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts tsx --watch src/payload/server.default.ts",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload build",
    "build:server": "tsc --project tsconfig.payload.json",
    "build:all": "yarn build:payload && yarn build:server",
    "payload:serve": "cross-env PAYLOAD_CONFIG_PATH=payload_dist/payload.config.js NODE_ENV=production node payload_dist/server.default.js",
    "copyfiles": "copyfiles -u 1 \"src/payload/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema",
    "payload:run": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload"
  },
  "exports": {
    "./setups": {
      "default": "./src/setups/index.ts"
    },
    "./exports": {
      "default": "./src/exports.ts"
    }
  },
  "devDependencies": {
    "@chakra-ui/cli": "next",
    "drizzle-kit": "^0.23.0",
    "@types/better-sqlite3": "^7.6.11",
    "@types/express": "^4.17.21"
  },
  "dependencies": {
    "@n2m/forms": "workspace:*",
    "@n2m/plugin-chakra": "workspace:*",
    "@n2m/plugin-effector": "workspace:*",
    "@n2m/shared-types": "workspace:*",
    "@n2m/cookies": "workspace:*",
    "@n2m/core-config": "workspace:*",
    "@n2m/adapter-effector": "workspace:*",
    "@n2m/shared-utils": "workspace:*",
    "@n2m/core-prerender": "workspace:*",
    "@n2m/router": "workspace:*",
    "@n2m/core-modules": "workspace:*",
    "@n2m/core-mocks": "workspace:*",
    "@n2m/core-di": "workspace:*",
    "@n2m/entry": "workspace:*",
    "@n2m/core-renderer": "workspace:*",
    "@n2m/shared-hooks": "workspace:*",
    "@n2m/core-ssr-vite": "workspace:*",
    "@n2m/plugin-gtag": "workspace:*",
    "@n2m/plugin-device": "workspace:*",
    "@n2m/plugin-unhead": "workspace:*",
    "@n2m/shared-tokens": "workspace:*",
    "@lucia-auth/adapter-sqlite": "^3.0.2",
    "drizzle-orm": "^0.32.1",
    "better-sqlite3": "^11.1.2",
    "lucia": "^3.2.0",
    "zod": "^3.23.8",
    "lucide-react": "^0.416.0",
    "drizzle-zod": "^0.5.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@neondatabase/serverless": "^0.9.4",
    "@payloadcms/db-postgres": "^0.8.5",
    "arctic": "^1.9.2",
    "dotenv": "^16.4.5",
    "@lucia-auth/adapter-postgresql": "^3.1.2",
    "cross-env": "^7.0.3",
    "react-icons": "^5.2.1",
    "qs": "^6.12.3",
    "escape-html": "^1.0.3",
    "react-spinners": "^0.14.1",
    "node-cron": "^3.0.2",
    "@lexical/clipboard": "0.13.1",
    "@lexical/code": "0.13.1",
    "@lexical/file": "0.13.1",
    "@lexical/hashtag": "0.13.1",
    "@lexical/headless": "0.13.1",
    "@lexical/html": "0.13.1",
    "@lexical/link": "0.13.1",
    "@lexical/list": "0.13.1",
    "@lexical/mark": "0.13.1",
    "@lexical/markdown": "0.13.1",
    "@lexical/overflow": "0.13.1",
    "@lexical/plain-text": "0.13.1",
    "@lexical/react": "0.13.1",
    "@lexical/rich-text": "0.13.1",
    "@lexical/selection": "0.13.1",
    "@lexical/table": "0.13.1",
    "@lexical/utils": "0.13.1",
    "@payloadcms/bundler-webpack": "^1.0.7",
    "@payloadcms/db-mongodb": "^1.5.1",
    "@payloadcms/live-preview-react": "^0.2.0",
    "@payloadcms/plugin-cloud": "^3.0.1",
    "@payloadcms/plugin-form-builder": "^1.2.2",
    "@payloadcms/plugin-nested-docs": "^1.0.12",
    "@payloadcms/plugin-redirects": "^1.0.1",
    "@payloadcms/plugin-seo": "^2.3.1",
    "@payloadcms/richtext-lexical": "^0.11.1",
    "express": "^4.19.2",
    "payload": "^2.20.0",
    "payload-admin-bar": "^1.0.6",
    "lexical": "0.13.1"
  }
}
